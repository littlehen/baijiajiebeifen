<script type="text/javascript">
    $(document).ready(function() {
        fans_ini();
        var is_auto = $_GET['is_auto'];
        var send_c_limit=$_GET['send_c_limit'];
        
        if(!send_c_limit || send_c_limit==0){
        	$('#bus_num').hide();
        }else{
        	$('#bus_num').show();
        	$('#bus_num span').html(send_c_limit+'家');
        }
        
        var openid = getCookie('openid');
        var datas = {
            'uniacid': get_uniacid(),
            openid: get_openid(),
            token:get_token(),
            'is_auto': is_auto,
            send_c_limit:send_c_limit
        };
        $('.mui-backdrop1').show();
        $.ajax({
            type: "post",
            url: api_borrow_apply(),
            dataType: 'json',
            async: true, 
            data: datas,
            success: function(res) {
            	console.log(res.data)
                if (res.status == 1) {
                	$('.mui-backdrop1').hide();
                    var data = res.data;
                    var lot_no = res.data.lot_no;
                     $('#pd').val(data.count)
                     $('.num').html($('#pd').val())
                     $(".num").numberRock({
					    speed:14,//数字越大数字滚动速度越慢
					    count:$('#pd').val()   //最终停留数字
					})
                     $('#pd').attr('vap','matchingbjtj.html?type_two=' + 0 + '&lot_no=' + lot_no)
                } else {
                	$('.mui-backdrop1').hide();
                    show_msg2("pop_i4", "close_i");
                    $('.pb15').text(res.info);
                }
            }
        });
		
		
	(function($) {
	    $.fn.numberRock = function(options) {
	        var defaults = {
	            speed: 24,
	            count: 100
	        };
	        var opts = $.extend({},
	        defaults, options);
	        var div_by = 100,
	        count = opts["count"],
	        speed = Math.floor(count / div_by),
	        sum = 0,
	        $display = this,
	        run_count = 1,
	        int_speed = opts["speed"];
	        var int = setInterval(function() {
	            if (run_count <= div_by && speed != 0) {
	                $display.text(sum = speed * run_count);
	                run_count++;
	            } else if (sum < count) {
	                $display.text(++sum);
	            } else {
	                clearInterval(int);
	            }
	        },
	        int_speed);
	    }
	
	})(jQuery);
	
	
	jqajax(get_wait_qrcode(), {'uniacid': get_uniacid()}, callback)

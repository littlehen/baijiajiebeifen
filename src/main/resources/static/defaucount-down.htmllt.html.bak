						}	
					}
					if(index.id==6){
						if(index.status==1){
							$('#button1').val(index.status).addClass('button2').html('申请').attr('img',index.qr_code)
						}else{
							$('#button1').val(index.status).addClass('button1').html('暂未开启')
						}	
					}
					if(index.id==8){
						if(index.status==1){
							$('#button3').val(index.status).addClass('button2').html('申请').attr('img',index.qr_code)
						}else{
							$('#button3').val(index.status).addClass('button1').html('暂未开启')
						}	
					}
					if(index.id==7){
						if(index.status==1){
							$('#button2').val(index.status).addClass('button2').html('申请').attr('img',index.qr_code)
						}else{
							$('#button2').val(index.status).addClass('button1').html('暂未开启')
						}	
					}
					if(index.id==9){
						if(index.status==1){
							$('#button5').val(index.status).addClass('button2').html('申请').attr('img',index.qr_code)
